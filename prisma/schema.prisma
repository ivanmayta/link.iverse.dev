datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
    output   = "../src/generated/client"
}

model Url {
    id          Int      @id @default(autoincrement())
    originalUrl String
    shortUrl    String
    createdAt   DateTime @default(now())
}
